{
  "name": "eslint-config-6river",
  "version": "0.0.0-development",
  "description": "ESLint shareable config for the 6RS style",
  "license": "Apache-2.0",
  "repository": "6RiverSystems/eslint-config-6river",
  "author": "6 River Systems",
  "engines": {
    "node": ">= 14.17.6"
  },
  "scripts": {
    "test": "node test/test.js"
  },
  "files": [
    "*.js"
  ],
  "keywords": [
    "6river",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint"
  ],
  "dependencies": {
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-6river": "^1.0.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.4"
  },
  "devDependencies": {
    "@6river/commitlint-config-6river": "^2.2.67",
    "@6river/prettier-config": "^1.0.59",
    "@commitlint/cli": "^16.2.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.2"
  },
  "peerDependencies": {
    "eslint": "^8.14.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@6river/commitlint-config-6river"
    ]
  }
}
