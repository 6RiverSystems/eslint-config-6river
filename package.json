{
  "name": "eslint-config-6river",
  "version": "0.0.0-development",
  "description": "ESLint shareable config for the 6RS style",
  "license": "Apache-2.0",
  "repository": "6RiverSystems/eslint-config-6river",
  "author": "6 River Systems",
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "test": "node test/test.js"
  },
  "files": [
    "*.js"
  ],
  "keywords": [
    "6river",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint"
  ],
  "dependencies": {
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-6river": "1.0.6",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-mocha": "^7.0.1"
  },
  "devDependencies": {
    "@6river/commitlint-config-6river": "^2.1.6",
    "@commitlint/cli": "^8.0.0",
    "eslint": "^7.2.0",
    "husky": "^4.0.0"
  },
  "peerDependencies": {
    "eslint": "^7.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@6river/commitlint-config-6river"
    ]
  }
}
